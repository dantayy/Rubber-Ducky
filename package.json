{
    "name": "rubber-ducky",
    "version": "1.0.0",
    "description": "Simple API to \"rubber-duck\" code problems on a public board",
    "main": "server.js",
    "scripts": {
        "build": "babel ./client --out-file ./hosted/bundle.js",
        "watch": "babel --watch ./client --out-file ./hosted/bundle.js",
        "nodemon": "nodemon --watch ./server ./server/server.js",
        "start": "node ./server/server.js",
        "pretest": "eslint ./server --fix",
        "test": "echo \"Tests complete\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dantayy/Rubber-Ducky.git"
    },
    "author": "Nicholas Mercadante",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dantayy/Rubber-Ducky/issues"
    },
    "homepage": "https://github.com/dantayy/Rubber-Ducky#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.18.2",
        "nodemon": "^1.19.3"
    }
}
